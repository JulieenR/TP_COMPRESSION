function [image_out] = calcmatQuantification(image_in,matQ)
     
    
    for i = 1:size(image_in)(3;
        image_intmp = image_in(:,:,i);
        if i==1
            image_out = round(image_intmp./matQ);
        else    
            image_out(:,:,i) = round(image_intmp./matQ);
        end
    end
end